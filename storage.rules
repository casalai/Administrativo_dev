rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Permitir acceso s√≥lo autenticado a adjuntos de solicitudes de ayuda
    match /help_requests/{allPaths=**} {
      allow read, write: if request.auth != null;
    }
    // Bloquear cualquier otra ruta por defecto
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}